<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>org.apache.ctakes.ade.types.TypeSystem</name>
    <description>This is a Apache cTAKES Type System for Adverse Drug Event detection.</description>
    <version>1.0</version>
    <vendor>Apache cTAKES</vendor>
    <imports>
        <import name="org.apache.ctakes.typesystem.types.TypeSystem" />
    </imports>
    <types>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.GeneMention</name>
            <description>A mention of a gene.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.MutationMention</name>
            <description>A mention of a mutation.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.CodonMention</name>
            <description>A mention of a codon.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.ExonMention</name>
            <description>A mention of an exon.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.ChromosomeMention</name>
            <description>A mention of a chromosome.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.SyntaxNMention</name>
            <description>A mention of a syntax n.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.SyntaxPMention</name>
            <description>A mention of a syntax p.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.RearrangmentMention</name>
            <description>A mention of a rearrangement.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>
        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.AmplificationMention</name>
            <description>A mention of an amplification.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
        </typeDescription>

        <typeDescription>
            <name>org.apache.ctakes.deepphe.clingen.types.textsem.InterpretationMention</name>
            <description>A mention of a gene.</description>
            <supertypeName>org.apache.ctakes.typesystem.type.textsem.IdentifiedAnnotation</supertypeName>
            <features>
            <featureDescription>
                <name>interpretation</name>
                <description>Is the gene positive, negative, indeterminate</description>
                <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
            </features>
        </typeDescription>


    </types>
</typeSystemDescription>